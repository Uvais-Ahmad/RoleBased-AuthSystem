<link href="/css/adminBackground.css" rel="stylesheet" type="text/css">

<table class="table table-striped table-dark w-75 mx-auto mt-5" style="margin-bottom:44vh">
    
    <thead>
        <tr>
            <th scope="col">Sr No.</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col" class="">Delete Emp</th>
        </tr>
    </thead>

    <tbody>
        <% let i=1; %> 
        <% for(user of users){ %>
            <% if(user.isAdmin){continue;} %> 
            <tr>
                <th scope="row"><%= i %> </th> 
                <td><%= user.name %> </td>
                <td><%= user.email %> </td>
                <td ><a role="button" href="/admin/deleteMember/<%= user._id %>" class="btn btn-danger">Delete</a></td>
                
            </tr>
            <% i++; %> 
        <% } %> 
        
    </tbody>
  </table>
